# SYNTAX TEST "Packages/zorgmode/zorgmode.sublime-syntax"

* Some headline
# <- org.text.headline.1
** Some headline level 2
# <- org.text.headline.2
*** Some headline level 3
# <- org.text.headline.3
**** Some headline level 4
# <- org.text.headline.4
***** Some headline level 5
# <- org.text.headline.5p
****** Some headline level 6
# <- org.text.headline.5p

*this is not headline
# <- org.text


Some text
# <- org.text

 - list entry
# <- org.text.list.entry

 * list entry
# <- org.text.list.entry

 1. list entry
# <- org.text.list.entry
 
 a. list entry
# <- org.text.list.entry

 * headline
# <- org.text.list.entry

[[some link]]
# ^^^^^^^^^ org.text.link.reference

- some text [[link]] [[link_address][text]]
#             ^^^^ org.text.link.reference
#                      ^^^^^^^^^^^^ org.text.link.reference
#                                    ^^^^ org.text.link.text
# ^^ org.text.list.entry

[[broken link]
* nonbroken headline
# ^ org.text.headline.1

#+ARCHIVE: foobar
# <- org.text.special_line
